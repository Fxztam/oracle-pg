CREATE PROPERTY GRAPH sh_purchase
  VERTEX TABLES (
    CUSTOMERS PROPERTIES (CUST_ID, CUST_FIRST_NAME),
    PRODUCTS PROPERTIES (PROD_ID, PROD_NAME)
  )
  EDGE TABLES (
    SALES_VIEW AS SALES_PURCHASED
      KEY (SALES_ID)
      SOURCE KEY (CUST_ID) REFERENCES CUSTOMERS
      DESTINATION KEY (PROD_ID) REFERENCES PRODUCTS
      LABEL PURCHASED
      NO PROPERTIES
  )
