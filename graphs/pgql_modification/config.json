{
  "format": "csv",
  "header": false,
  "vertex_labels":true,
  "edge_label":true,
  "vertex_uris": ["vertices.csv"],
  "edge_uris": ["edges.csv"],
  "vertex_props": [
    { "name": "age", "type": "integer" },
    { "name": "name", "type": "string" },
    { "name": "profession", "type": "string" },
    { "name": "carOwner", "type": "boolean" },
    { "name": "weight", "type": "integer" }
  ],
  "edge_props": [
    { "name": "since", "type": "local_date" }
  ],
  "loading_options": {
    "load_vertex_labels":true,
    "load_edge_label":true
  }
}
